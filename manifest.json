{
  "manifest_version": 3,
  "name": "Neko Login",
  "version": "1.0",
  "description": "Login kawaii no Roblox via cookie!",
  "permissions": ["cookies", "storage", "tabs", "scripting"],
  "host_permissions": ["https://www.roblox.com/"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}